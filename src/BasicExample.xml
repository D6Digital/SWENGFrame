			<point num="00" x="500" y="400"></point>
		</shape>
		<shape lineColour="#FF0000" totalpoints="2" fillcolour="#FFFFFF"
			layer="00" branch="0" duration="20" height="50" starttime="10"
			width="50">
			<point num="00" x="100" y="550"></point>
			<point num="00" x="350" y="550"></point>
		</shape>
		<audio urlname="src/XMLBits/03 The First Steps.mp3" starttime="0" loop="true"></audio>
		<image urlname="src/XMLBits/char1jovian_spy.jpg" xstart="10" ystart="10"
			width="282" height="463" starttime="0" duration="100" layer="1"
			branch="1"></image>
	</slide>
	<slide id="2" duration="240" lastSlide="true">
		<text font="Arial" fontsize="40" fontcolour="#FF9500" starttime="20"
			duration="100" layer="0" xstart="50" ystart="50" xend="400" yend="200">
			<textbody bold="false" italic="false" underlined="true"
				branch="0">This is a title. Its underlined and everything.</textbody>
			<textbody bold="false" italic="false" underlined="false"
				branch="01">CLICK ME</textbody>
		</text>
		<video urlname="/XMLBits/worlds_shittest_video.avi" startTime="10"
			loop="true" xstart="20" ystart="23" layer="1"></video>
	</slide>
	
</slideshow>

