		<title>Characters</title>
		<comment>A slideshow with information about characters</comment>
		<width>720</width>
		<height>640</height>
	</documentinfo>
	<defaults>
		<backgroundcolour>#FFFFFF</backgroundcolour>
		<font>Times New Roman</font>
		<fontsize>24</fontsize>
		<fontcolour>#000000</fontcolour>
		<linecolour>#000000</linecolour>
		<fillcolour>#000000</fillcolour>
	</defaults>
	<slide id="1" duration="60" lastSlide="false">
		<text font="Arial" fontsize="36" fontcolour="#291F9F" starttime="5"
			duration="25" layer="0" xstart="300" ystart="200" xend="700" yend="230">
			<textbody bold="true" italic="false" underlined="false"
				branch="0">Jovian Spy</textbody>
		</text>
		<text font="Arial" fontsize="25" fontcolour="#291F9F" starttime="5"
			duration="25" layer="0" xstart="300" ystart="250" xend="700" yend="640">
