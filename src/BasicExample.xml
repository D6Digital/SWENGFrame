<slideshow>
	<documentinfo>
		<author>Joshua Lant Andrew Walter</author>
		<version>2.0</version>
		<title>Basic Example</title><comment>An example slideshow.</comment>
		<width>640</width>
		<height>480</height>
		</documentinfo>
	<defaults>
		<backgroundcolour>#000000</backgroundcolour>
		<font>Times New Roman</font>
		<fontsize>24</fontsize>
		<fontcolour>#FFFFFF</fontcolour>
		<linecolour>#FFFFFF</linecolour>
		<fillcolour>#FFFFFF</fillcolour></defaults>
	<slide id="1" duration="60" lastSlide="false">
		<text font="Arial" fontsize="36" fontcolour="#FF9500" starttime="5" duration="25" layer="0" xstart="100" ystart="100" xend="400" yend="400">
			<textbody bold="false" italic="false" underlined="false" branch="0">This is the first slide of the basic example.</textbody>
			<textbody bold="true" italic="false" underlined="false" branch="0">This should be in bold.</textbody></text>
		<shape totalpoints="3" width="10" height="10" fillcolour="#0000FF" starttime="10" duration="30" layer="2" lineColour="#FFFFFF" branch="0">
			<point num="00" x="10" y="10"></point>
			<point num="01" x="15" y="15"></point>
			<point num="02" x="10" y="15"></point></shape>
		<audio urlname="src/XMLBits/RunWithUs.mp3" starttime="30" loop="true"></audio>
		<image urlname="/images/revelation.png" xstart="40" ystart="200" width="400" height="300" starttime="0" duration="100" layer="1" branch="0"></image>
		</slide>
			<slide id="2" duration="240" lastSlide="true">
		<text font="Arial" fontsize="40" fontcolour="#FF9500" starttime="20" duration="100"
			layer="0" xstart="50" ystart="50" xend="400" yend="200">
			<textbody bold="false" italic="false" underlined="true" branch="0">This is a title. Its underlined and everything.</textbody>
			<textbody bold="false" italic="false" underlined="false" branch="01">CLICK ME</textbody>
		</text>
		<video urlname="/XMLBits/worlds_shittest_video.avi" startTime="10" loop="true" xstart="20" ystart="23" layer="1"></video></slide>
		</slideshow>